<body class="with-content-panel">
<div class="all-wrapper menu-side with-pattern">
    <div class="auth-box-w">
        <div class="logo-w">


            <!--<img alt="" src="../static/img/Icon.png" height="150" width="150">-->

        </div>
        <h4 class="auth-header">Login </h4>
        <form name="loginForm"  novalidate>
            <div class="alert alert-warning alert-dismissible fade show displayerror" role="alert" style="display:none">
                <button aria-label="Close" class="close"  type="button">
                    <span aria-hidden="true"> &times;</span>
                </button>
                <strong>{{authorizationMessage}} </strong>
            </div>
            <div class="form-group" >
                <label >Wallet ID</label>
                <input type="text" name="walletId" class="form-control" placeholder="Enter Your Wallet ID..." id="walletId" ng-maxlength="40" required>
                <div class="pre-icon os-icon os-icon-user-male-circle"></div>
                <!--<p class="text-danger" ng-show="loginForm.walletId.$invalid && !loginForm.walletId.$pristine"> Wallet ID Is Required</p>
                <p class="text-danger" ng-show="loginForm.walletId.$error.maxlength"> Wallet ID Is Too Long</p>-->
            </div>
            <div class="form-group">
                <label>Password</label>
                <input type="password" name="password" class="form-control" placeholder="Enter Your Password... " id="password" ng-minlength="3" ng-maxlength="20" required>
                <div class="pre-icon os-icon os-icon-fingerprint"></div>
                <!--<p class="text-danger" ng-show="loginForm.password.$invalid && !loginForm.password.$pristine"> Password Is Required</p>-->
            </div>
            <div class="buttons-w">
                <button type="button" class="btn btn-primary" id="loginButton" button-spinner="loading" spinner-icon="fa fa-spinner">
                    <span ng-hide="loading">Sign in to your account</span>
                    <span ng-show="loading">Decrypting your wallet</span>
                </button>
                <div class="form-check-inline">
                    <label class="form-check-label">
                        <a href="#!/auth/register">Register</a>

                    </label>

                </div>
            </div>
            <div class="float-lg-right buttons-w">
                <a href="./"><i class="fa fa-2x fa-home"></i></a>
            </div>
        </form>
    </div>
</div>

